{
  "name": "react-dreams",
  "version": "0.2.0",
  "description": "",
  "main": "server/index.js",
  "author": "Paul Borrego <paul.borrego@turner.com>",
  "dependencies": {
    "express": "4.13.4",
    "react": "15.0.2",
    "react-dom": "15.0.2",
    "redux": "3.5.2"
  },
  "scripts": {
    "clean": "rm -r dist && mkdir -p dist/public",
    "build": "parallelshell -v \"npm run build:transpile\" \"npm run build:resources\"",
    "build:transpile": "babel src --out-dir dist",
    "build:resources": "rsync -a --exclude='*.js' ./src/ ./dist",
    "watch": "parallelshell -v \"npm run watch:transpile\" \"npm run watch:resources\"",
    "watch:transpile": "babel src --watch --out-dir dist",
    "watch:resources": "nodemon --ext scss,css,jpg,gif,svg --exec \"rsync -a --exclude='*.js' ./src/ ./dist\"",
    "start:server": "node dist/server",
    "start": "npm run clean && npm run build && parallelshell -v \"npm run start:server\" \"npm run watch\""
  },
  "devDependencies": {
    "babel-cli": "6.9.0",
    "babel-core": "6.9.0",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-object-assign": "6.8.0",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "chokidar": "1.5.1",
    "css-loader": "0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "node-sass": "3.7.0",
    "nodemon": "1.9.2",
    "parallelshell": "2.0.0",
    "react-hot-loader": "3.0.0-beta.2",
    "sass-loader": "3.2.0",
    "style-loader": "0.13.1",
    "webpack": "1.13.0",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.10.0"
  },
  "license": "MIT"
}
